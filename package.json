{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "Boilerplate for react using webpack",
  "scripts": {
    "bundle": "env NODE_ENV=production NODE_PATH=. i18n-util run webpack",
    "i18n": "npm run build && npm run i18n:export && npm run i18n:import",
    "i18n:export": "i18n-util export --project $POEDITOR_PROJECT_ID",
    "i18n:import": "i18n-util import --project $POEDITOR_PROJECT_ID",
    "lint": "npm-run-parallel lint:css lint:js",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:js": "eslint $npm_package_config_jslintables",
    "lint:fix": "npm run lint:js:fix",
    "lint:js:fix": "eslint --fix $npm_package_config_jslintables",
    "start": "env NODE_PATH=$PWD NODE_ENV=development STANDALONE_MODE=true node ./lib/dev-server",
    "test": "npm-run-parallel lint test:unit",
    "test:unit": "env NODE_ENV=test NODE_PATH=. karma start karma.conf.js",
    "test:unit:watch": "npm run test:unit -- --single-run=false",
    "watch": "npm run test:unit:watch"
  },
  "config": {
    "jslintables": "src lib webpack.config.js karma.conf.js"
  },
  "author": "Swapnil Chaudhari",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-object-assign": "^1.2.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "css": "^2.2.1",
    "css-loader": "^0.23.1",
    "dirty-chai": "^1.2.2",
    "dotenv": "^2.0.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-mocha": "^4.0.0",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "i18n-util": "git+ssh://git@github.com/theorchard/js-util-poeditor.git",
    "ignore-styles": "^4.0.0",
    "import-glob-loader": "^1.1.0",
    "imports-loader": "^0.6.5",
    "intl": "^1.2.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.0",
    "karma-coverage": "^1.1.0",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-notify-reporter": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "loader": "^2.1.1",
    "lodash.assign": "^4.2.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.13.1",
    "npm-run-parallel": "^0.5.0",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^0.9.1",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-intl": "^2.2.3",
    "react-modal-bootstrap": "^1.1.1",
    "react-redux": "^5.0.4",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.9.0",
    "style-loader": "^0.13.2",
    "stylelint": "^6.9.0",
    "webpack": "^1.15.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "yarn": "^0.22.0"
  },
  "dependencies": {
    "babel-plugin-react-intl": "^2.1.3",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "intl": "^1.2.4",
    "lodash": "^4.17.4",
    "mocha": "^2.5.3",
    "react": "^15.2.0",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.2.0",
    "react-intl": "^2.1.3",
    "sinon": "^1.17.7",
    "yarn": "^0.22.0"
  }
}
